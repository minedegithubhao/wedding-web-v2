{"remainingRequest":"/Users/cxdpc/WorkSpaces/idea_workspace/wedding-web/node_modules/vue-loader/lib/index.js??vue-loader-options!/Users/cxdpc/WorkSpaces/idea_workspace/wedding-web/src/views/pts/member/modules/HunCustomListModal.vue?vue&type=script&lang=js&","dependencies":[{"path":"/Users/cxdpc/WorkSpaces/idea_workspace/wedding-web/src/views/pts/member/modules/HunCustomListModal.vue","mtime":1605796384434},{"path":"/Users/cxdpc/WorkSpaces/idea_workspace/wedding-web/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/cxdpc/WorkSpaces/idea_workspace/wedding-web/node_modules/babel-loader/lib/index.js","mtime":315532800000},{"path":"/Users/cxdpc/WorkSpaces/idea_workspace/wedding-web/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/cxdpc/WorkSpaces/idea_workspace/wedding-web/node_modules/vue-loader/lib/index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["HunCustomListModal.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAqMA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"HunCustomListModal.vue","sourceRoot":"src/views/pts/member/modules","sourcesContent":["<template>\n  <a-modal title=\"操作\"\n           style=\"top: 20px;\"\n           :width=\"1200\"\n           :body-style=\"{ padding: 0, height: '80vh' }\"\n           v-model=\"visible\"\n           :confirmLoading=\"confirmLoading\"\n           @ok=\"handleSubmit\">\n    <v-scroll>\n      <div style=\"padding: 24px\">\n        <div class=\"table-page-search-wrapper\">\n          <a-form layout=\"inline\">\n            <a-row :gutter=\"48\">\n              <a-col :md=\"4\"\n                     :sm=\"15\">\n                <a-form-item label=\"姓名\">\n                  <a-input placeholder=\"请输入姓名\"\n                           v-model=\"queryParam.name\" />\n                </a-form-item>\n              </a-col>\n              <a-col :md=\"4\"\n                     :sm=\"15\">\n                <a-form-item label=\"学历\">\n                  <a-select placeholder=\"学历\"\n                            v-model=\"queryParam.education\">\n                    <a-select-option value=\"\">所有</a-select-option>\n                    <a-select-option v-for=\"(b, index) in educationArray\"\n                                     :key=\"index\"\n                                     :value=\"b.dictValue\">{{ b.dictLabel }}</a-select-option>\n                  </a-select>\n                </a-form-item>\n              </a-col>\n              <template v-if=\"advanced\">\n                <a-col :md=\"4\"\n                       :sm=\"15\">\n                  <a-form-item label=\"民族\">\n                    <a-input placeholder=\"请输入民族\"\n                             suffix=\"族\"\n                             v-model=\"queryParam.nation\" />\n                  </a-form-item>\n                </a-col>\n                <a-col :md=\"4\"\n                       :sm=\"15\">\n                  <a-form-item label=\"籍贯\">\n                    <a-input placeholder=\"请输入籍贯\"\n                             v-model=\"queryParam.nativePlace\" />\n                  </a-form-item>\n                </a-col>\n                <a-col :md=\"4\"\n                       :sm=\"15\">\n                  <a-form-item label=\"身高\">\n                    <a-input placeholder=\"请输入身高\"\n                             suffix=\"CM\"\n                             v-model=\"queryParam.height\" />\n                  </a-form-item>\n                </a-col>\n                <a-col :md=\"4\"\n                       :sm=\"15\">\n                  <a-form-item label=\"体重\">\n                    <a-input placeholder=\"请输入体重\"\n                             suffix=\"KG\"\n                             v-model=\"queryParam.weight\" />\n                  </a-form-item>\n                </a-col>\n                <a-col :md=\"4\"\n                       :sm=\"15\">\n                  <a-form-item label=\"职业\">\n                    <a-input placeholder=\"请输入职业\"\n                             v-model=\"queryParam.job\" />\n                  </a-form-item>\n                </a-col>\n                <a-col :md=\"4\"\n                       :sm=\"15\">\n                  <a-form-item label=\"工作地址\">\n                    <a-input placeholder=\"请输入工作地址\"\n                             v-model=\"queryParam.jobAddr\" />\n                  </a-form-item>\n                </a-col>\n                <a-col :md=\"4\"\n                       :sm=\"15\">\n                  <a-form-item label=\"家庭住址\">\n                    <a-input placeholder=\"请输入家庭住址\"\n                             v-model=\"queryParam.homeAddr\" />\n                  </a-form-item>\n                </a-col>\n                <a-col :md=\"4\"\n                       :sm=\"15\">\n                  <a-form-item label=\"是否有房\">\n                    <a-select placeholder=\"是否有房\"\n                              v-model=\"queryParam.house\">\n                      <a-select-option value=\"\">所有</a-select-option>\n                      <a-select-option v-for=\"(b, index) in houseArray\"\n                                       :key=\"index\"\n                                       :value=\"b.dictValue\">{{ b.dictLabel }}</a-select-option>\n                    </a-select>\n                  </a-form-item>\n                </a-col>\n                <a-col :md=\"4\"\n                       :sm=\"15\">\n                  <a-form-item label=\"是否有车\">\n                    <a-select placeholder=\"是否有车\"\n                              v-model=\"queryParam.car\">\n                      <a-select-option value=\"\">所有</a-select-option>\n                      <a-select-option v-for=\"(b, index) in carArray\"\n                                       :key=\"index\"\n                                       :value=\"b.dictValue\">{{ b.dictLabel }}</a-select-option>\n                    </a-select>\n                  </a-form-item>\n                </a-col>\n                <a-col :md=\"4\"\n                       :sm=\"15\">\n                  <a-form-item label=\"血型\">\n                    <a-select placeholder=\"血型\"\n                              v-model=\"queryParam.blood\">\n                      <a-select-option value=\"\">所有</a-select-option>\n                      <a-select-option v-for=\"(b, index) in bloodArray\"\n                                       :key=\"index\"\n                                       :value=\"b.dictValue\">{{ b.dictLabel }}</a-select-option>\n                    </a-select>\n                  </a-form-item>\n                </a-col>\n                <a-col :md=\"4\"\n                       :sm=\"15\">\n                  <a-form-item label=\"工作状态\">\n                    <a-select placeholder=\"工作状态\"\n                              v-model=\"queryParam.status\">\n                      <a-select-option value=\"\">所有</a-select-option>\n                      <a-select-option v-for=\"(b, index) in statusArray\"\n                                       :key=\"index\"\n                                       :value=\"b.dictValue\">{{ b.dictLabel }}</a-select-option>\n                    </a-select>\n                  </a-form-item>\n                </a-col>\n                <a-col :md=\"4\"\n                       :sm=\"15\">\n                  <a-form-item label=\"婚姻状况\">\n                    <a-select placeholder=\"婚姻状况\"\n                              v-model=\"queryParam.maritalStatus\">\n                      <a-select-option value=\"\">所有</a-select-option>\n                      <a-select-option v-for=\"(b, index) in maritalStatusArray\"\n                                       :key=\"index\"\n                                       :value=\"b.dictValue\">{{ b.dictLabel }}</a-select-option>\n                    </a-select>\n                  </a-form-item>\n                </a-col>\n              </template>\n              <a-col :md=\"8\"\n                     :sm=\"24\">\n                <span class=\"table-page-search-submitButtons\">\n                  <a-button type=\"primary\"\n                            @click=\"$refs.table.refresh(true)\">查询</a-button>\n                  <a-button style=\"margin-left: 8px\"\n                            @click=\"() => queryParam = {}\">重置</a-button>\n                  <a @click=\"advanced = !advanced\"\n                     style=\"margin-left: 8px\">\n                    {{ advanced ? '收起' : '展开' }}\n                    <a-icon :type=\"advanced ? 'up' : 'down'\" />\n                  </a>\n                </span>\n              </a-col>\n            </a-row>\n          </a-form>\n        </div>\n        <s-table size=\"default\"\n                 ref=\"table\"\n                 rowKey=\"id\"\n                 :rowSelection=\"{selectedRowKeys: selectedRowKeys, onChange: onSelectChange}\"\n                 :columns=\"columns\"\n                 :data=\"loadData\">\n          <span slot=\"nation\"\n                slot-scope=\"text\">{{ text ? `${text}族` : '未知' }}</span>\n          <span slot=\"height\"\n                slot-scope=\"text\">{{ text ? `${text}CM` : '未知' }}</span>\n          <span slot=\"weight\"\n                slot-scope=\"text\">{{ text ? `${text}KG` : '未知' }}</span>\n          <span slot=\"sex\"\n                slot-scope=\"text\">{{ text | dictFilter(sexMap) }}</span>\n          <span slot=\"education\"\n                slot-scope=\"text\">{{ text | dictFilter(educationMap) }}</span>\n          <span slot=\"maritalStatus\"\n                slot-scope=\"text\">{{ text | dictFilter(maritalStatusMap) }}</span>\n          <span slot=\"house\"\n                slot-scope=\"text\">{{ text | dictFilter(houseMap) }}</span>\n          <span slot=\"car\"\n                slot-scope=\"text\">{{ text | dictFilter(carMap) }}</span>\n          <span slot=\"status\"\n                slot-scope=\"text\">\n            <a-badge :status=\"text | statusTypeFilter\"\n                     :text=\"text | dictFilter(statusMap)\" />\n          </span>\n        </s-table>\n      </div>\n    </v-scroll>\n  </a-modal>\n</template>\n\n<script>\nimport { STable } from '@/components'\nimport { getHunCustomList } from '@/api/pts/custom/custom'\nimport { checkPermission } from '@/utils/permissions'\nimport { getDictArray } from '@/utils/dict'\n\nexport default {\n  name: 'HunCustomListModal',\n  components: {\n    STable\n  },\n  data() {\n    return {\n      visible: false,\n      confirmLoading: false,\n      labelCol: {\n        xs: { span: 24 },\n        sm: { span: 5 }\n      },\n      wrapperCol: {\n        xs: { span: 24 },\n        sm: { span: 16 }\n      },\n      form: this.$form.createForm(this),\n      mdl: {},\n      // 高级搜索 展开/关闭\n      advanced: false,\n      // 查询参数\n      queryParam: {},\n      // 表头\n      columns: [\n        {\n          title: '编号',\n          dataIndex: 'id'\n        },\n        {\n          title: '姓名',\n          dataIndex: 'name'\n        },\n        {\n          title: '姓别',\n          dataIndex: 'sex',\n          scopedSlots: { customRender: 'sex' },\n          sorter: true\n        },\n        {\n          title: '民族',\n          dataIndex: 'nation',\n          scopedSlots: { customRender: 'nation' },\n          sorter: true\n        },\n        {\n          title: '出生日期',\n          dataIndex: 'birthday',\n          sorter: true\n        },\n        {\n          title: '身高',\n          dataIndex: 'height',\n          scopedSlots: { customRender: 'height' },\n          sorter: true\n        },\n        {\n          title: '体重',\n          dataIndex: 'weight',\n          scopedSlots: { customRender: 'weight' },\n          sorter: true\n        },\n        {\n          title: '学历',\n          dataIndex: 'education',\n          scopedSlots: { customRender: 'education' },\n          sorter: true\n        },\n        {\n          title: '婚姻状况',\n          dataIndex: 'maritalStatus',\n          scopedSlots: { customRender: 'maritalStatus' },\n          sorter: true\n        },\n        {\n          title: '是否有房',\n          dataIndex: 'house',\n          scopedSlots: { customRender: 'house' },\n          sorter: true\n        },\n        {\n          title: '是否有车',\n          dataIndex: 'car',\n          scopedSlots: { customRender: 'car' },\n          sorter: true\n        },\n        {\n          title: '工作状态',\n          dataIndex: 'status',\n          scopedSlots: { customRender: 'status' },\n          sorter: true\n        }\n      ],\n      // 加载数据方法 必须为 Promise 对象\n      loadData: (parameter) => {\n        return getHunCustomList(Object.assign(parameter, this.queryParam))\n      },\n      selectedRowKeys: [],\n      selectedRows: [],\n      statusArray: [],\n      statusMap: {},\n      relationArray: [],\n      relationMap: {}\n    }\n  },\n  filters: {\n    dictFilter(dataIndex, Map) {\n      return Map[dataIndex].text\n    },\n    statusTypeFilter(status) {\n      const statusMap = {\n        '1': 'success',\n        '2': 'error',\n        '3': 'warning',\n        '4': 'processing',\n        '5': 'purple',\n        '6': 'default'\n      }\n      return statusMap[status] || 'default'\n    }\n  },\n  created() {\n    this.init()\n  },\n  methods: {\n    init() {\n      getDictArray('yes_no').then(data => {\n        this.houseArray = data\n        this.carArray = data\n        const dataMap = {}\n        data.map(d => {\n          dataMap[d.dictValue] = { text: d.dictLabel }\n        })\n        this.houseMap = dataMap\n        this.carMap = dataMap\n      })\n      getDictArray('hun_custom_blood').then(data => {\n        this.bloodArray = data\n        const bloodMap = {}\n        this.bloodArray.map(d => {\n          bloodMap[d.dictValue] = { text: d.dictLabel }\n        })\n        this.bloodMap = bloodMap\n      })\n      getDictArray('hun_custom_marital').then(data => {\n        this.maritalStatusArray = data\n        const maritalStatusMap = {}\n        this.maritalStatusArray.map(d => {\n          maritalStatusMap[d.dictValue] = { text: d.dictLabel }\n        })\n        this.maritalStatusMap = maritalStatusMap\n      })\n      getDictArray('hun_custom_work_status').then(data => {\n        this.statusArray = data\n        const statusMap = {}\n        this.statusArray.map(d => {\n          statusMap[d.dictValue] = { text: d.dictLabel }\n        })\n        this.statusMap = statusMap\n      })\n      getDictArray('hun_custom_education').then(data => {\n        this.educationArray = data\n        const educationMap = {}\n        this.educationArray.map(d => {\n          educationMap[d.dictValue] = { text: d.dictLabel }\n        })\n        this.educationMap = educationMap\n      })\n      getDictArray('sys_sex').then(data => {\n        this.sexArray = data\n        const sexMap = {}\n        this.sexArray.map(d => {\n          sexMap[d.dictValue] = { text: d.dictLabel }\n        })\n        this.sexMap = sexMap\n      })\n    },\n    onSelectChange(selectedRowKeys, selectedRows) {\n      this.selectedRowKeys = selectedRowKeys\n      this.selectedRows = selectedRows\n    },\n    handleOpen(param) {\n      this.selectedRowKeys = []\n      this.selectedRows = []\n      // this.queryParam.sex = param.sex\n      this.visible = true\n      if (this.$refs.table) {\n        this.$refs.table.refresh(true)\n      }\n    },\n    handleSubmit() {\n      if (this.selectedRowKeys.length === 1) {\n        this.$emit('ok', this.selectedRowKeys[0], this.selectedRows[0])\n        this.visible = false\n      } else {\n        this.$message.warning('请选择待配对者')\n      }\n    }\n  }\n}\n</script>\n"]}]}