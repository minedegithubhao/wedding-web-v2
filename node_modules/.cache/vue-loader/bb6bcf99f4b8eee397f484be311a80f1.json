ute-view></route-view>\n\n      <div class=\"footer\">\n        <logo class=\"global-logo login-footer-logo\"\n              :show-title=\"device !== 'mobile'\"\n              :title=\"logoTitle\" />\n        <!-- <div class=\"links\">\n          <a href=\"/portalClient/index\"\n             target=\"_blank\">首页</a>\n          <a href=\"/portalClient/script/index\"\n             target=\"_blank\">指纹验证</a>\n          <a href=\"/dashboard/welcome\"\n             target=\"_blank\">系统管理</a>\n          <a href=\"/portalClient/special/index\"\n             target=\"_blank\">专项服务</a>\n        </div> -->\n        <div class=\"copyright\">\n          Copyright\n          <a-icon type=\"copyright\" />2020\n          <span>WEDDING</span>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport RouteView from './RouteView'\nimport { mixinDevice } from '@/utils/mixin'\nimport Logo from '@/components/tools/Logo'\n\nexport default {\n  name: 'UserLayout',\n  components: { RouteView, Logo },\n  mixins: [mixinDevice],\n  data() {\n    return {\n      logoTitle: '婚介管理平台'\n    }\n  },\n  mounted() {\n    document.body.classList.add('userLayout')\n  },\n  beforeDestroy() {\n    document.body.classList.remove('userLayout')\n  }\n}\n</script>\n<style lang=\"less\">\n#userLayout.user-layout-wrapper {\n  height: 100vh;\n\n  &.mobile {\n    .container {\n      .main {\n        .user-layout-login {\n          float: none;\n          margin-right: 0;\n          margin: 0 auto;\n        }\n      }\n    }\n  }\n}\n</style>\n<style lang=\"less\" scoped>\n#userLayout.user-layout-wrapper {\n  height: 100vh;\n\n  &.mobile {\n    .container {\n      .main {\n        width: 98%;\n      }\n    }\n  }\n\n  .container {\n    width: 100%;\n    background: url(~@/assets/login/bg.jpg) no-repeat top/cover;\n    padding: 4vh 0 144px;\n    position: relative;\n\n    a {\n      text-decoration: none;\n    }\n\n    .top {\n      width: 100%;\n      text-align: left;\n      padding: 0 4%;\n      height: 100px;\n      margin: 0 auto;\n\n      .header {\n        // height: 44px;\n        line-height: 60px;\n\n        .badge {\n          position: absolute;\n          display: inline-block;\n          line-height: 1;\n          vertical-align: middle;\n          margin-left: -12px;\n          margin-top: -10px;\n          opacity: 0.8;\n        }\n\n        .logo {\n          height: 60px;\n          vertical-align: top;\n          // margin-right: 16px;\n          border-style: none;\n          margin-bottom: 18px;\n        }\n        .dot {\n          display: block;\n          margin-left: 18px;\n        }\n        .title {\n          font-size: 26px;\n          color: #fff;\n          font-family: '微软雅黑';\n          // font-weight: 600;\n          position: relative;\n          top: 2px;\n          letter-spacing: 2px;\n          margin-left: 8px;\n          // word-spacing: ;\n        }\n        .title_left {\n          color: #556ee6;\n        }\n      }\n      .desc {\n        font-size: 14px;\n        color: #fff;\n        margin-top: 12px;\n        margin-bottom: 40px;\n      }\n    }\n\n    .main {\n      // min-width: 1200px;\n      width: 100%;\n      height: calc(100vh - 100px - 5vh - 4vh - 144px);\n      min-height: 480px;\n      padding: 4% 2% 0;\n      margin-top: 5vh;\n      position: relative;\n      &::before {\n        content: '';\n        display: block;\n        position: absolute;\n        left: 2%;\n        top: 0;\n        width: calc(600px * (933 / 638));\n        height: 600px;\n        background: url(~@/assets/login/left.png) no-repeat center/contain;\n      }\n    }\n    // @media only screen and (min-height: 500px) {\n    //   .main {\n    //     margin-top: 2vh;\n    //     height: calc(100vh - 100px - 2vh - 4vh - 144px);\n    //   }\n    // }\n    // @media only screen and (min-height: 700px) {\n    //   .main {\n    //     margin-top: 5vh;\n    //     height: calc(100vh - 100px - 5vh - 4vh - 144px);\n    //   }\n    // }\n    // @media only screen and (min-height: 850px) {\n    //   .main {\n    //     margin-top: 7vh;\n    //     height: calc(100vh - 100px - 7vh - 4vh - 144px);\n    //   }\n    // }\n    .footer {\n      position: absolute;\n      width: 100%;\n      bottom: 0;\n      padding: 0 16px;\n      margin: 48px 0 24px;\n      text-align: center;\n\n      .links {\n        margin-bottom: 8px;\n        font-size: 14px;\n        a {\n          color: #fff;\n          transition: all 0.3s;\n          &:not(:last-child) {\n            margin-right: 40px;\n          }\n        }\n      }\n      .copyright {\n        color: #fff;\n        font-size: 14px;\n      }\n    }\n  }\n}\n</style>\n"]}]}