opupContainer=\"() => $refs.noticeRef.parentElement\"\n             :autoAdjustOverflow=\"true\"\n             :arrowPointAtCenter=\"true\"\n             :overlayStyle=\"{ width: '300px', top: '50px' }\">\n    <template slot=\"content\">\n      <a-spin :spinning=\"loading\">\n        <a-tabs>\n          <a-tab-pane tab=\"通知\"\n                      key=\"1\">\n            <a-list>\n              <a-list-item>\n                <a-list-item-meta title=\"通知一\"\n                                  description=\"一个小时前\">\n                  <a-avatar style=\"background-color: white\"\n                            slot=\"avatar\"\n                            :src=\"one\" />\n                </a-list-item-meta>\n              </a-list-item>\n              <a-list-item>\n                <a-list-item-meta title=\"通知二\"\n                                  description=\"一天前\">\n                  <a-avatar style=\"background-color: white\"\n                            slot=\"avatar\"\n                            :src=\"two\" />\n                </a-list-item-meta>\n              </a-list-item>\n              <a-list-item>\n                <a-list-item-meta title=\"通知三\"\n                                  description=\"两天前\">\n                  <a-avatar style=\"background-color: white\"\n                            slot=\"avatar\"\n                            :src=\"three\" />\n                </a-list-item-meta>\n              </a-list-item>\n            </a-list>\n          </a-tab-pane>\n          <a-tab-pane tab=\"消息\"\n                      key=\"2\">\n            123\n          </a-tab-pane>\n          <a-tab-pane tab=\"待办\"\n                      key=\"3\">\n            123\n          </a-tab-pane>\n        </a-tabs>\n      </a-spin>\n    </template>\n    <span @click=\"fetchNotice\"\n          class=\"header-notice\"\n          ref=\"noticeRef\"\n          style=\"padding: 0 18px\">\n      <a-badge count=\"12\"\n               :numberStyle=\"{height:'12px',lineHeight:'12px',padding:'0 4px'}\">\n        <a-icon style=\"font-size: 20px; padding: 0 4px\"\n                type=\"bell\" />\n      </a-badge>\n    </span>\n  </a-popover>\n</template>\n\n<script>\nimport one from '@/assets/icons/ThXAXghbEsBCCSDihZxY.png'\nimport two from '@/assets/icons/OKJXDXrmkNshAMvwtvhu.png'\nimport three from '@/assets/icons/kISTdvpyTAhtGxpovNWd.png'\nexport default {\n  name: 'HeaderNotice',\n  data() {\n    return {\n      loading: false,\n      visible: false,\n      one: one,\n      two: two,\n      three: three\n    }\n  },\n  methods: {\n    fetchNotice() {\n      if (!this.visible) {\n        this.loading = true\n        setTimeout(() => {\n          this.loading = false\n        }, 2000)\n      } else {\n        this.loading = false\n      }\n      this.visible = !this.visible\n    }\n  }\n}\n</script>\n\n<style lang=\"css\">\n.header-notice-wrapper {\n  top: 50px !important;\n}\n</style>\n<style lang=\"less\" scoped>\n.header-notice {\n  display: inline-block;\n  transition: all 0.3s;\n\n  span {\n    vertical-align: middle;\n  }\n}\n</style>\n"]}]}