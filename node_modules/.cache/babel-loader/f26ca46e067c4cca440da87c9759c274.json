ble-page-search-wrapper\">\n      <a-form layout=\"inline\">\n        <a-row :gutter=\"48\">\n          <a-col :md=\"5\"\n                 :sm=\"15\">\n            <a-form-item label=\"部门名称\">\n              <a-input placeholder=\"请输入\"\n                       v-model=\"queryParam.deptName\" />\n            </a-form-item>\n          </a-col>\n          <a-col :md=\"5\"\n                 :sm=\"15\">\n            <a-form-item label=\"状态\">\n              <a-select placeholder=\"请选择\"\n                        v-model=\"queryParam.status\">\n                <a-select-option value>全部</a-select-option>\n                <a-select-option value=\"0\">正常</a-select-option>\n                <a-select-option value=\"1\">停用</a-select-option>\n              </a-select>\n            </a-form-item>\n          </a-col>\n          <a-col :md=\"8\"\n                 :sm=\"24\">\n            <span class=\"table-page-search-submitButtons\">\n              <a-button type=\"primary\"\n                        @click=\"this.fetch\">查询</a-button>\n              <a-button style=\"margin-left: 8px\"\n                        @click=\"() => queryParam = {}\">重置</a-button>\n            </span>\n          </a-col>\n        </a-row>\n      </a-form>\n    </div>\n    <div class=\"table-operator\">\n      <a-button v-if=\"addEnable\"\n                type=\"primary\"\n                icon=\"plus\"\n                @click=\"$refs.modal.add()\">新建</a-button>\n    </div>\n    <a-table ref=\"table\"\n             rowKey=\"deptId\"\n             :pagination=\"pagination\"\n             :loading=\"loading\"\n             :columns=\"columns\"\n             :expand-row-by-click=\"true\"\n             :dataSource=\"data\">\n      <span slot=\"menuType\"\n            slot-scope=\"text\">{{ text | menuTypeFilter }}</span>\n\n      <span slot=\"status\"\n            slot-scope=\"text\">{{ text | statusFilter }}</span>\n\n      <span slot=\"action\"\n            slot-scope=\"text, record\">\n        <a-icon type=\"edit\"\n                v-if=\"editEnabel\"\n                class=\"icon-action edit\"\n                @click=\"handleEdit(record)\" />\n        <a-icon type=\"plus\"\n                v-if=\"addEnable\"\n                class=\"icon-action add\"\n                @click=\"handleAdd(record.deptId+'')\" />\n        <a-popconfirm title=\"确认删除?\"\n                      v-if=\"removeEnable\"\n                      @confirm=\"() => delById(record.deptId)\">\n          <a-icon type=\"delete\"\n                  class=\"icon-action delete\" />\n        </a-popconfirm>\n      </span>\n    </a-table>\n\n    <dept-modal ref=\"modal\"\n                @ok=\"handleOk\" />\n  </a-card>\n</template>\n\n<script>\nimport { Table as T } from 'ant-design-vue'\nimport { getDeptList, delDept } from '@/api/system'\nimport DeptModal from './modules/DeptModal.vue'\nimport { treeData } from '@/utils/treeutil'\nimport { checkPermission } from '@/utils/permissions'\nexport default {\n  name: 'TableList',\n  components: {\n    T,\n    DeptModal\n  },\n  data() {\n    return {\n      description: '列表使用场景：后台管理中的权限管理以及角色管理，可用于基于 RBAC 设计的角色权限控制，颗粒度细到每一个操作类型。',\n\n      visible: false,\n      labelCol: {\n        xs: { span: 24 },\n        sm: { span: 5 }\n      },\n      wrapperCol: {\n        xs: { span: 24 },\n        sm: { span: 16 }\n      },\n      form: null,\n      mdl: {},\n      // 高级搜索 展开/关闭\n      advanced: false,\n      // 查询参数\n      queryParam: {},\n      // 表头\n      columns: [\n        {\n          title: '权限名称',\n          dataIndex: 'deptName'\n        },\n        {\n          title: '排序',\n          dataIndex: 'orderNum'\n        },\n        {\n          title: '状态',\n          dataIndex: 'status',\n          scopedSlots: { customRender: 'status' }\n        },\n        {\n          title: '创建时间',\n          dataIndex: 'createTime'\n        },\n        {\n          title: '操作',\n          width: '120px',\n          dataIndex: 'action',\n          align: 'center',\n          scopedSlots: { customRender: 'action' }\n        }\n      ],\n      data: [],\n      pagination: false,\n      loading: false,\n      addEnable: checkPermission('system:dept:add'),\n      editEnabel: checkPermission('system:dept:edit'),\n      removeEnable: checkPermission('system:dept:remove')\n    }\n  },\n  filters: {\n    statusFilter(status) {\n      const statusMap = {\n        '1': '停用',\n        '0': '正常'\n      }\n      return statusMap[status]\n    },\n    menuTypeFilter(type) {\n      const menuMap = {\n        'M': '目录',\n        'F': '按钮',\n        'C': '菜单'\n      }\n      return menuMap[type]\n    }\n  },\n  created() {\n    this.fetch()\n  },\n  methods: {\n    handleAdd(parentId) {\n      this.$refs.modal.add(parentId)\n    },\n    handleEdit(record) {\n      this.$refs.modal.edit(record)\n    },\n    handleOk() {\n      this.fetch()\n    },\n    delById(id) {\n      delDept(id).then(res => {\n        if (res.code === 0) {\n          this.$message.success(`删除成功`)\n          this.handleOk()\n        } else {\n          this.$message.error(res.msg)\n        }\n      })\n    },\n    fetch() {\n      this.loading = true\n      getDeptList(Object.assign(this.queryParam)).then(res => {\n        this.data = treeData(res.rows, 'deptId')\n        this.loading = false\n      })\n    }\n  },\n  watch: {\n    /*\n      'selectedRows': function (selectedRows) {\n        this.needTotalList = this.needTotalList.map(item => {\n          return {\n            ...item,\n            total: selectedRows.reduce( (sum, val) => {\n              return sum + val[item.dataIndex]\n            }, 0)\n          }\n        })\n      }\n      */\n  }\n}\n</script>\n"],"sourceRoot":"src/views/system"}]}