  <a-form :form=\"form\">\n      <a-row :gutter=\"64\">\n        <a-col :span=\"12\"\n               style=\"border-right: 1px solid #eaeaea\">\n          <div class=\"modal-divider-text\">基本配置</div>\n          <a-form-item style=\"display:none\">\n            <a-input v-decorator=\"['userId']\" />\n          </a-form-item>\n          <a-form-item :labelCol=\"labelCol\"\n                       :wrapperCol=\"wrapperCol\"\n                       label=\"用户名\">\n            <a-input placeholder=\"用户名\"\n                     v-decorator=\"['loginName', {rules: [{ required: true, message: '请输入用户名'}]}]\" />\n          </a-form-item>\n\n          <a-form-item :labelCol=\"labelCol\"\n                       :wrapperCol=\"wrapperCol\"\n                       label=\"昵称\">\n            <a-input v-decorator=\"[\n                'userName',\n                {\n                  rules: [{ required: true, message: '请输入昵称' }]\n                }\n              ]\"\n                     placeholder=\"起一个名字\" />\n          </a-form-item>\n\n          <a-form-item :labelCol=\"labelCol\"\n                       :wrapperCol=\"wrapperCol\"\n                       label=\"状态\">\n            <a-select v-decorator=\"['status', {initialValue:'0',rules: [{ required: true, message: '请选择状态' }]}]\">\n              <a-select-option :value=\"'0'\">正常</a-select-option>\n              <a-select-option :value=\"'1'\">禁用</a-select-option>\n            </a-select>\n          </a-form-item>\n\n          <a-form-item :labelCol=\"labelCol\"\n                       :wrapperCol=\"wrapperCol\"\n                       label=\"描述\">\n            <a-textarea :rows=\"5\"\n                        placeholder=\"...\"\n                        v-decorator=\"['remark', {rules: [{ required: false, message: '请输入描述' }]}]\" />\n          </a-form-item>\n        </a-col>\n        <a-col :span=\"12\">\n          <div class=\"modal-divider-text\">高级配置</div>\n          <a-form-item :labelCol=\"labelCol\"\n                       :wrapperCol=\"wrapperCol\"\n                       label=\"部门\">\n            <a-tree-select v-decorator=\"['deptId', {rules: [{ required: true, message: '请选择部门' }]}]\"\n                           :dropdownStyle=\"{ maxHeight: '400px', overflow: 'auto' }\"\n                           :treeData=\"deptTree\"\n                           @change=\"onChange\"\n                           placeholder=\"部门\"\n                           treeDefaultExpandAll></a-tree-select>\n          </a-form-item>\n          <a-form-item :labelCol=\"labelCol\"\n                       :wrapperCol=\"wrapperCol\"\n                       label=\"拥有角色\"\n                       hasFeedback>\n            <a-select style=\"width: 100%\"\n                      mode=\"multiple\"\n                      v-decorator=\"['roleIds', {rules: [{ required: true, message: '请选择角色' }]}]\"\n                      :allowClear=\"true\">\n              <a-select-option v-for=\"(action) in roleAll\"\n                               :key=\"action.roleId\">{{ action.roleName }}</a-select-option>\n            </a-select>\n          </a-form-item>\n        </a-col>\n      </a-row>\n    </a-form>\n  </a-modal>\n</template>\n<script>\nimport { getRoleAll, saveUser } from '@/api/system'\nimport pick from 'lodash.pick'\nexport default {\n  name: 'UserModal',\n  props: {\n    deptTree: {\n      type: Array,\n      required: true\n    }\n  },\n  components: {},\n  data() {\n    return {\n      description:\n        '列表使用场景：后台管理中的权限管理以及角色管理，可用于基于 RBAC 设计的角色权限控制，颗粒度细到每一个操作类型。',\n      visible: false,\n      labelCol: {\n        xs: { span: 24 },\n        sm: { span: 6 }\n      },\n      wrapperCol: {\n        xs: { span: 24 },\n        sm: { span: 17 }\n      },\n      confirmLoading: false,\n      roleAll: [],\n      mdl: {},\n      deptCheck: true,\n      form: this.$form.createForm(this)\n    }\n  },\n  beforeCreate() { },\n  created() {\n    this.loadRoleAll()\n  },\n  methods: {\n    add() {\n      this.form.resetFields()\n      this.edit({ userId: 0, deptId: '' })\n    },\n    edit(record) {\n      this.mdl = Object.assign({}, record)\n      this.visible = true\n      this.$nextTick(() => {\n        this.form.setFieldsValue(\n          pick(this.mdl, 'userId', 'loginName', 'userName', 'status', 'roleIds', 'remark', 'deptId')\n        )\n        // this.form.setFieldsValue({ ...record })\n      })\n    },\n    onChange(value, label, extra) {\n      if (extra.triggerNode.$children.length > 0) {\n        this.$message.error('不能选择父节点' + `${label}`)\n        this.deptCheck = false\n      } else {\n        this.deptCheck = true\n      }\n    },\n    loadRoleAll() {\n      getRoleAll().then(res => {\n        this.roleAll = res.rows\n      })\n    },\n    handleSubmit(e) {\n      e.preventDefault()\n      if (!this.deptCheck) {\n        this.$message.error('不能选择父节点')\n        return\n      }\n      this.form.validateFields((err, values) => {\n        if (!err) {\n          this.confirmLoading = true\n          saveUser(values)\n            .then(res => {\n              if (res.code === 0) {\n                this.$message.success('保存成功')\n                this.$emit('ok')\n                this.visible = false\n              } else {\n                this.$message.success(res.msg)\n              }\n            })\n            .catch(() => {\n              this.$message.error('系统错误，请稍后再试')\n            })\n            .finally(() => {\n              this.confirmLoading = false\n            })\n        }\n      })\n    }\n  },\n  watch: {\n    /*\n      'selectedRows': function (selectedRows) {\n        this.needTotalList = this.needTotalList.map(item => {\n          return {\n            ...item,\n            total: selectedRows.reduce( (sum, val) => {\n              return sum + val[item.dataIndex]\n            }, 0)\n          }\n        })\n      }\n      */\n  }\n}\n</script>\n"],"sourceRoot":"src/views/system/modules"}]}